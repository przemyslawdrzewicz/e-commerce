{
    "root": true,
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "@nuxtjs/eslint-config-typescript", 
      "plugin:vue/vue3-recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": ["vue"],
    "rules": {
      "prettier/prettier": ["error", { "endOfLine": "auto" }],
      "vue/multi-word-component-names": "off"
    }
}